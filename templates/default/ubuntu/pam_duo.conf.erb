[duo]
; Duo integration key
ikey = <%= node['duo']['keys']['integration'] %>
; Duo secret key
skey = <%= node['duo']['keys']['secret'] %>
; Duo API hostname
host = <%= node['duo']['keys']['host'] %>
; Local groups and exclusions
<% if node['duo']['groups'] %>
groups = <%= node['duo']['groups'] %>
<% end %>
pushinfo = <%= node['duo']['pushinfo'] %>
autopush = <%= node['duo']['autopush'] %>
